#!/bin/bash

# Initialize command
command="exit"

# Clean up
stop_shell

# Run commands
echo "$command" | "$SHELL" &
echo "$?" > "$YOUR_OUTPUT" 2> "$YOUR_ERROR"

echo "$command" | sh
echo "$?" > "$EXPECTED_OUTPUT" 2> "$EXPECTED_ERROR"

# Check output and errors
check_output
check_error

# Clean up again
stop_shell
