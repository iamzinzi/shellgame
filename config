# Configurations

TESTDIR="."
R=$RANDOM
YOUR_OUTPUT="your_output.txt"
EXPECTED_OUTPUT="expected_output.txt"
YOUR_OUTPUT_SORTED="your_output_sorted.txt"
EXPECTED_OUTPUT_SORTED="expected_output_sorted.txt"
OUTPUT_DIFF="output_diff.txt"
YOUR_ERROR="your_error.txt"
EXPECTED_ERROR="expected_error.txt"
YOUR_ERROR_SORTED="your_error_sorted.txt"
EXPECTED_ERROR_SORTED="expected_error_sorted.txt"
ERROR_DIFF="error_diff.txt"
SHELL="./hsh"
SLEEP=`which sleep`
SLEEPSECONDS=1
LTRACE=`which ltrace`
LTRACEOUTPUTFILE="checker_ltrace_output_$R"

# Pass/fail color scheme
RED="\e[31m"
GREEN="\e[92m"
RESET="\e[0m"
